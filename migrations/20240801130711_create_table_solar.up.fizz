create_table("solar") {
    t.Column("id", "integer", {primary: true})
    t.Column("plan", "string")
    t.Column("land_area_minimum", "integer")
    t.Column("land_area_maxmimum", "integer")
    t.Column("power", "string")
    t.Column("modules", "string")
    t.Column("batteries", "string")
    t.Column("accessories", "string")
    t.Column("electricity_annual", "string")
    t.Column("company", "string")
}
